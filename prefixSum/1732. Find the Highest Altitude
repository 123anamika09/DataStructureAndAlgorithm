class Solution {
    public int largestAltitude(int[] gain) {
          int max=0;
        int initialaltitude=0;
        for(int i=0;i<gain.length;i++){
            initialaltitude=initialaltitude+gain[i];
             if(initialaltitude>max){
                max=initialaltitude;
             }
        }
        return max;
    }
}
