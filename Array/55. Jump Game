class Solution {
    public boolean canJump(int[] nums) {
        //  Greedy algo
        int maxIdx = 0;
        int n = nums.length-1;
        for(int i = 0;i<=n;i++){
            if(i>maxIdx) return false;
            maxIdx = Math.max(maxIdx, i+nums[i]);
            if(maxIdx>=n) return true;
        }
        return false;

    }
}
