class MyStack {
   Queue<Integer> q1  ;
     Queue<Integer> q2;
    public MyStack() {
     q1  = new LinkedList<>();
     q2 = new LinkedList<>();
    }
    
    public void push(int x) {
        //  costly push - push = o(n) & pop = o(1)
        q2.add(x); // add x to stack 
        while(!q1.isEmpty()) {
           q2.add(q1.remove());
        }
        Queue<Integer> temp = q1;
        q1 = q2;
        q2 = temp;
    }

    
    public int pop() {
        
        return q1.remove();
    }

    public int top() {
     return q1.peek();
    }
    
    public boolean empty() {
        return q1.isEmpty();
    }
}
